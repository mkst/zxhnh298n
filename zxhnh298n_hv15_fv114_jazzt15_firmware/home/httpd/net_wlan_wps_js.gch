<script language="javascript">var WPS_PARA = new Array("Enable","WPSMode","WpsMethodSupported","APSetupLocked","WPSState","ConfigurationError","DeviceName","DevicePasswordID","OOBDevicePassword","NumberOfAssociatedRegistrars","RegistrarEstablished","UUID","Version");function getmsg(id){var str = new Array();str[0] = new Array(101, "&?error_101;");str[1] = new Array(543, "&?error_543;");str[2] = new Array(544, "&?error_544;");return getMsgFormArray(str, arguments)}function isOnlyContainNum(value, Frm, Fnt){var len = value.length;var ch = "";for (var i=0; i<len; i++){ch = value.charAt(i);if (!((ch>='0')&&(ch<='9'))){msg = getmsg(543);ShowErrorForCom(Frm,Fnt,msg);return false;}}return true;}function checkPinCode(value, Frm, Fnt){if (value.length == 0){msg = getmsg(101);ShowErrorForCom(Frm,Fnt,msg);return false;}if (value.length != 8){msg = getmsg(544);ShowErrorForCom(Frm, Fnt, msg);return false;}if (isOnlyContainNum(value, Frm, Fnt) == false){return false;}return true;}function Show_PINMode(){var pinMode = getValue("Frm_PINMode");if (pinMode == "InputPIN"){jslSetValue("Frm_InputPINVal", "IF_InputPINVal");jslEnDisplay("Tr_InputPIN");jslDiDisplay("Tr_AutoPIN");}else {jslSetValue("Frm_AutoPINVal", "IF_AutoPINVal");jslDiDisplay("Tr_InputPIN");jslEnDisplay("Tr_AutoPIN");}}function Slect_WPSMode(){HiddenParaInit(WPS_PARA);var wpsmode = getValue("Frm_WPSMode");if (wpsmode == "Disabled"){setValue("Enable", "0");}else if (wpsmode == "PBC"){setValue("Enable", "1");setValue("WPSMode", "0");}else {setValue("Enable", "1");setValue("WPSMode", "1");}setValue("IF_ACTION", "WPSMode");jslDisable("Frm_WPSMode","Frm_PINMode","Frm_AutoPIN","Frm_InputPIN","Frm_InputPINVal");getObj("fSubmit").submit();}function AutoPINAction(){setValue("IF_PIN_OP", "AutoPIN");setValue("IF_ACTION", "AutoPIN");jslDisable("Frm_WPSMode", "Frm_PINMode", "Frm_AutoPIN", "Frm_InputPIN");getObj("fSubmit").submit();}function InputPINAction(){var pin = getValue("Frm_InputPINVal");if (checkPinCode(pin, "Frm_InputPINVal", "Fnt_InputPINVal") == true){setValue("IF_InputPINVal", pin);setValue("IF_PIN_OP", "InputPIN");setValue("IF_ACTION", "InputPIN");jslDisable("Frm_WPSMode", "Frm_PINMode", "Frm_InputPIN");getObj("fSubmit").submit();}}function Slect_PINMode(){var pinOp = getValue("Frm_PINMode");if (pinOp == "AutoPIN"){AutoPINAction();}else {jslEnDisplay("Tr_InputPIN");jslDiDisplay("Tr_AutoPIN");}}function Wireless_switch(){var RealRF = getValue("RealRF");if( RealRF== "1"){getObj("tipLi").innerHTML = "&?cwps8;&?pub000;";jslEnDisplay("t_warn");jslEnable("Frm_WPSMode","Frm_PINMode","Frm_AutoPIN","Frm_InputPIN","Frm_InputPINVal");}else {getObj("tipLi").innerHTML = "&?wlan064;&?pub002;";jslEnDisplay("t_warn");jslDisable("Frm_WPSMode","Frm_PINMode","Frm_AutoPIN","Frm_InputPIN","Frm_InputPINVal");}}function pageLoad(url){getObj("fSubmit").action = url;var errstr= getValue("IF_ERRORSTR");var errpara = getValue("IF_ERRORPARAM");OldProcessRet(errstr, errpara);pageGetValue();moveErrLayer(570,4);}function pageGetValue(){var enable = getValue("Enable");if (enable == "0"){setValue("Frm_WPSMode", "Disabled");}else {var wpsmode = getValue("WPSMode");if (wpsmode == "0"){setValue("Frm_WPSMode", "PBC");}else {setValue("Frm_WPSMode", "PIN");jslEnDisplay("Tr_PINMode");var pinOp = getValue("IF_PIN_OP");if (pinOp=="AutoPIN"|| pinOp=="InputPIN"){setValue("Frm_PINMode", pinOp);}else {setValue("Frm_PINMode", "AutoPIN");}Show_PINMode();}}Wireless_switch();}</script>