<%IMPORT FILE "common_gch.gch";function wepkey_func(FP_ACTION, FP_TAG, FP_INDEX, CONFIG_IDENTITY, FP_ERRORSTR){var FP_OBJNAME = "OBJ_WLANWEPKEY_ID";var FP_MULTIDISPLAY = 1;var FP_PARANUM = 1;var FP_IDENTITY = "";var FP_HANDLE;var PARA[2] ={"WEPKey","WEPKey"};var FP_INSTNUM = query_list(FP_OBJNAME, CONFIG_IDENTITY);if (FP_INSTNUM != 888 && FP_INSTNUM >0 && FP_INDEX >=0 && FP_ACTION != "new"){FP_IDENTITY = query_identity(FP_INDEX);}if ("Y"== FP_TAG && ("apply"== FP_ACTION || "new"== FP_ACTION )){FP_HANDLE = create_paralist();for(var i=0; i< FP_PARANUM; i++){set_multiinst_para(FP_HANDLE, PARA[i], FP_INDEX);}FP_IDENTITY = set_inst(FP_HANDLE, FP_OBJNAME, FP_IDENTITY);if ("new"== FP_ACTION){FP_ERRORSTR = getNewInstError(FP_ERRORSTR);if ("SUCC"!= FP_ERRORSTR){FP_INDEX = -1;FP_IDENTITY = "";}}}query_list(FP_OBJNAME, CONFIG_IDENTITY);if (FP_MULTIDISPLAY == 1){for (var i=0; i<4; i++){FP_HANDLE = create_paralist();FP_IDENTITY = query_identity(i);get_inst(FP_HANDLE, FP_OBJNAME, FP_IDENTITY);set_hidden_multiinstpara(FP_HANDLE, PARA, i, FP_PARANUM);destroy_paralist(FP_HANDLE);}}return getDisplayInstError(FP_ERRORSTR);}%>